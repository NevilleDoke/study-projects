    "#BLACK JACK\n",
    "def blackjack(a, b, c):\n",
    "    if sum([a, b, c]) <= 21:\n",
    "        return sum([a, b, c])\n",
    "    elif 11 in [a, b, c] and sum([a, b, c]) <= 31:\n",
    "        return (sum([a, b, c]) - 10)\n",
    "    else:\n",
    "        return 'BUST'"
